{% extends 'uniSistemasBundle::layout.html.twig' %}


{% block cuerpo -%}
    <h1>hardware</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Ipreal</th>
                <td>{{ entity.ipreal }}</td>
            </tr>
            <tr>
                <th>Memoria</th>
                <td>{{ entity.memoria }}</td>
            </tr>
            <tr>
                <th>Disco</th>
                <td>{{ entity.disco }}</td>
            </tr>
            <tr>
                <th>Cpu</th>
                <td>{{ entity.cpu }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('hardware') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('hardware_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
